{
  "version": "0.2.0",
  "configurations": [
      {
        "name": "debug streamlit app",
        "type": "debugpy",
        "request": "launch",
        "module": "streamlit",
        "args": [
          "run",
          "home.py"
        ],
        "cwd": "${workspaceFolder}/src/app"
      },
      {
        "name": "load tpch_sf100 parallel, large",
        "type": "debugpy",
        "request": "launch",
        "module": "load_from_csv",
        "args": [
          "--warehouse", "load",
          "--size", "LARGE",
          "--database", "meetup_gddp",
          "--mode", "parallel",
          "--tables_definition", "${workspaceFolder}/tools/snowpark/tpch_sf100.json"],
        "cwd": "${workspaceFolder}/tools/snowpark/"
      },
      {
        "name": "load tpch_sf100 parallel, medium",
        "type": "debugpy",
        "request": "launch",
        "module": "load_from_csv",
        "args": [
          "--warehouse", "load",
          "--size", "medium",
          "--database", "meetup_gddp",
          "--mode", "parallel",
          "--tables_definition", "${workspaceFolder}/tools/snowpark/tpch_sf100.json"],
        "cwd": "${workspaceFolder}/tools/snowpark/"
      },
      {
        "name": "load tpch_sf100 sequential, large",
        "type": "debugpy",
        "request": "launch",
        "module": "load_from_csv",
        "args": [
          "--warehouse", "load",
          "--size", "LARGE",
          "--database", "meetup_gddp",
          "--mode", "sequential",
          "--tables_definition", "${workspaceFolder}/tools/snowpark/tpch_sf100.json"],
        "cwd": "${workspaceFolder}/tools/snowpark/"
      }

  ]
  }
